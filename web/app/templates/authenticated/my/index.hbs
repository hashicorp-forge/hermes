{{page-title "Recent"}}

{{log "my index model" this.model}}
<ol class="tile-list">
  {{#each this.model as |doc|}}
    <li>
      <Doc::Tile
        @avatar={{get doc.ownerPhotos 0}}
        @docID={{doc.objectID}}
        @docNumber={{doc.docNumber}}
        @isDraft={{doc.isDraft}}
        @modifiedTime={{doc.modifiedTime}}
        @owner={{get doc.owners 0}}
        @productArea={{doc.product}}
        @title={{doc.title}}
        @status={{doc.status}}
      />
    </li>
  {{/each}}
</ol>
