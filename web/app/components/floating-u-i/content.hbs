{{#maybe-in-element
  (html-element ".ember-application")
  this.shouldRenderInPlace
  insertBefore=null
}}
  <div
    {{will-destroy this.cleanup}}
    {{did-insert this.didInsert}}
    data-test-floating-ui-placement={{@placement}}
    id="popover-{{this.id}}"
    class="hermes-popover"
    ...attributes
  >
    {{yield}}
  </div>
{{/maybe-in-element}}
