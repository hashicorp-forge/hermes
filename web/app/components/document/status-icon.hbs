<svg
  data-test-document-status-icon
  data-test-status={{@status}}
  xmlns="http://www.w3.org/2000/svg"
  fill="none"
  viewBox="0 0 20 20"
  class="w-5"
>
  {{#if this.isObsolete}}
    {{! page fill with unpeeled right corner }}
    <path
      data-test-obsolete-page-fill
      fill="#fff"
      d="M2 2.2A2.2 2.2 0 0 1 4.2 0h11.6A2.2 2.2 0 0 1 18 2.2v15.6a2.2 2.2 0 0 1-2.2 2.2H4.2A2.2 2.2 0 0 1 2 17.8V2.2Z"
    />
    {{! folder fill }}
    <path
      data-test-obsolete-folder-fill
      fill="#DEDFE3"
      fill-rule="evenodd"
      d="M12.959 10.497a1 1 0 0 0-.521.878V20H4.2A2.2 2.2 0 0 1 2 17.8V2.2A2.2 2.2 0 0 1 4.2 0h8.444a2.2 2.2 0 0 1 2.142 1.695v6.618a2 2 0 0 1-1.043 1.756l-.784.428Z"
      clip-rule="evenodd"
    />
  {{else}}
    {{! page fill with peeled right corner }}
    <path
      data-test-default-page-fill
      fill="#fff"
      fill-rule="evenodd"
      d="M1.5 2.25A2.25 2.25 0 0 1 3.75 0h9.293c.331 0 .65.132.884.366l4.207 4.207c.234.235.366.553.366.884V17.75A2.25 2.25 0 0 1 16.25 20H3.75a2.25 2.25 0 0 1-2.25-2.25V2.25Z"
      clip-rule="evenodd"
    />
  {{/if}}

  {{#if this.defaultStrokeIsShown}}
    {{! page stroke }}
    <path
      data-test-default-page-stroke
      data-test-status={{@status}}
      fill="#ADB0B8"
      fill-rule="evenodd"
      d="M3.75 0A2.25 2.25 0 0 0 1.5 2.25v15.5A2.25 2.25 0 0 0 3.75 20h12.5a2.25 2.25 0 0 0 2.25-2.25V5.457c0-.331-.132-.65-.366-.884L13.927.366A1.25 1.25 0 0 0 13.043 0H3.75ZM3 2.25a.75.75 0 0 1 .75-.75h8.75v3.75c0 .414.336.75.75.75H17v11.75a.75.75 0 0 1-.75.75H3.75a.75.75 0 0 1-.75-.75V2.25ZM15.94 4.5 14 2.56V4.5h1.94Zm-10.19.212a.75.75 0 0 0 0 1.5h4.098a.75.75 0 1 0 0-1.5H5.75Zm0 5.764a.75.75 0 0 0 0 1.5h8.098a.75.75 0 0 0 0-1.5H5.75ZM5 14.108a.75.75 0 0 1 .75-.75h5.633a.75.75 0 0 1 0 1.5H5.75a.75.75 0 0 1-.75-.75Zm.75-6.514a.75.75 0 1 0 0 1.5h2.47a.75.75 0 0 0 0-1.5H5.75Z"
      clip-rule="evenodd"
    />
    {{#if this.isDraft}}
      {{! pencil }}
      <path
        data-test-pencil-icon
        fill="#dedfe3"
        stroke="#8c909c"
        stroke-linejoin="round"
        stroke-width="1.5"
        d="m15.433 8.697 2.005 2.004-5.162 5.162h-2.004v-2.004l5.161-5.162Z"
      />
    {{/if}}
  {{else if this.isApproved}}
    {{! page stroke }}
    <path
      data-test-approved-page-stroke
      fill="#ADB0B8"
      fill-rule="evenodd"
      d="M3.75 0A2.25 2.25 0 0 0 1.5 2.25v15.5A2.25 2.25 0 0 0 3.75 20h12.5a2.25 2.25 0 0 0 2.25-2.25V5.457c0-.331-.132-.65-.366-.884L13.927.366A1.25 1.25 0 0 0 13.043 0H3.75ZM3 2.25a.75.75 0 0 1 .75-.75h8.75v3.75c0 .414.336.75.75.75H17v11.75a.75.75 0 0 1-.75.75H3.75a.75.75 0 0 1-.75-.75V2.25ZM15.94 4.5 14 2.56V4.5h1.94Zm-10.19.212a.75.75 0 0 0 0 1.5h4.098a.75.75 0 1 0 0-1.5H5.75Zm0 2.882a.75.75 0 1 0 0 1.5h2.47a.75.75 0 0 0 0-1.5H5.75Z"
      clip-rule="evenodd"
    />
    {{! checkmark }}
    <path
      data-test-checkmark-icon
      stroke="#008A22"
      stroke-linecap="round"
      stroke-linejoin="round"
      stroke-width="1.5"
      d="m8.5 12.558 2.031 2.905 3.971-6.369"
    />
  {{else if this.isObsolete}}
    {{! page and folder stroke }}
    <path
      data-test-obsolete-page-stroke
      fill="#ADB0B8"
      fill-rule="evenodd"
      d="M15.8 1.5h-1.07c.021.064.04.129.056.195v6.618a2 2 0 0 1-1.043 1.756l-.784.428a1 1 0 0 0-.521.878V18.5H15.8a.7.7 0 0 0 .7-.7V2.2a.7.7 0 0 0-.7-.7Zm-12.296.628A.7.7 0 0 1 4.2 1.5h8.444c.286 0 .533.172.642.419v6.394a.5.5 0 0 1-.261.439l-.785.428a2.5 2.5 0 0 0-1.303 2.195V18.5H4.2a.7.7 0 0 1-.7-.7v-5.812h2.813a.75.75 0 0 0 0-1.5H3.5V9.094H9a.75.75 0 0 0 0-1.5H3.5V6.2H9a.75.75 0 0 0 0-1.5H3.5V2.2c0-.024.001-.048.004-.072ZM15.8 0H4.2A2.2 2.2 0 0 0 2 2.2v15.6A2.2 2.2 0 0 0 4.2 20h11.6a2.2 2.2 0 0 0 2.2-2.2V2.2A2.2 2.2 0 0 0 15.8 0Z"
      clip-rule="evenodd"
    />
  {{/if}}
</svg>
