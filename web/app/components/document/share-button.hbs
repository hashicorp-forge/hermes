<Hds::Dropdown @listPosition="left" as |dd|>
  <dd.ToggleIcon ...attributes @icon="share" @text="share" @hasChevron={{false}} />
  <dd.CopyItem
    @text="{{this.shortLinkBaseURL}}/{{lowercase @document.docType}}/{{lowercase
      @document.docNumber
    }}"
    @copyItemTitle="Copy URL"
  />
</Hds::Dropdown>
