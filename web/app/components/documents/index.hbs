{{#if @facets}}
  {{#in-element (html-element ".header-nav-and-filters") insertBefore=null}}
    <Header::Toolbar @facets={{@facets}} />
  {{/in-element}}
{{/if}}

<RowResults
  @docs={{@results.hits}}
  @nbPages={{@results.nbPages}}
  @currentPage={{(add @results.page 1)}}
  @currentSort="createdTime"
  @sortDirection={{this.sortDirection}}
/>
